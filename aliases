alias ls='clear; ls -G'
alias la="clear; ls -a"
alias ll="clear; ls -l"
alias lc='clear; ls'
alias c='clear'

alias code='cd ~/Documents/Code'
alias cse='cd ~/Dropbox/CSE'
alias dotfiles='cd ~/Documents/dotfiles'
alias box='cd ~/Dropbox'
alias reload='source ~/.bash_profile'
alias gocode='cd ~/Documents/Code/go_ws/src/github.com/mkmathur'

alias attu='ssh mkmathur@attu.cs.washington.edu'

#tl: list sessions
alias tl='tmux ls'

#tn <name>: create a session named <name>
alias tn='tmux -2 new -s'

#ta <name>: attach to a session named <name>
alias ta='tmux -2 attach -d -t'

alias notify='terminal-notifier -message "DONE"'
