[user]
  name = Mallika Mathur
  email = mallika@mkmathur.com
[core]
	excludesfile = /Users/mkmathur/.gitignore_global
[push]
  default = simple
[alias]
  last = log -1 HEAD
  st = status
  s = status -s
  co = checkout
  br = branch
  ci = commit
  did = commit -m
  cam = commit -a -m
  discard = checkout --
  branches = branch -a
  stashes = stash list
  unstage = reset -q HEAD --
  uncommit = reset --mixed HEAD~
  amend = commit --amend
  precommit = diff --cached --diff-algorithm=minimal -w
[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
[color]
	ui = auto
