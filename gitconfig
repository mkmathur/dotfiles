[user]
  name = Mallika Mathur
	email = mkmathur@users.noreply.github.com
[core]
	excludesfile = /Users/mkmathur/.gitignore_global
[push]
	default = current
[alias]
  l = log --oneline
  last = log -1 HEAD
  st = status
  sst = status
  s = status -s
  co = checkout
  br = branch
  ci = commit
  did = commit -m
  cam = commit -a -m
  discard = checkout --
  branches = branch
  stashes = stash list
  unstage = reset -q HEAD --
  uncommit = reset --mixed HEAD~
  amend = commit --amend
  precommit = diff --cached --diff-algorithm=minimal -w
  pf = push --force
  staged = diff --staged
[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
[color]
	ui = auto
[rebase]
	autosquash = true
